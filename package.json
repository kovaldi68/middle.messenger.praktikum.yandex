{
  "name": "messenger-shi-no-ka",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "parcel src/index.html --no-source-maps --no-cache --port 3000",
    "build": "parcel build src/index.html --no-cache",
    "server": "npm run build && node server.js",
    "lint": "eslint --debug src/",
    "lint-fix": "eslint --debug src/ --fix"
  },
  "staticFiles": {
    "staticPath": "src/static"
  },
  "alias": {
    "core": "./src/core",
    "pages": "./src/pages",
    "components": "./src/components"
  },
  "engines": {
    "node": "16.15.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@parcel/transformer-sass": "2.7.0",
    "@types/node": "18.8.3",
    "eslint": "8.22.0",
    "eslint-plugin-prettier": "4.2.1",
    "handlebars": "4.7.7",
    "parcel": "2.7.0",
    "parcel-reporter-static-files-copy": "1.4.0",
    "sass": "1.54.4"
  },
  "dependencies": {
    "babel-core": "6.26.3",
    "express": "4.18.1",
    "nanoid": "4.0.0",
    "typescript": "4.8.4"
  },
  "targets": {
    "browserModern": {
      "engines": {
        "browsers": [
          "last 1 Chrome version"
        ]
      }
